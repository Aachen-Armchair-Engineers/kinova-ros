    <xacro:include filename="$(find kinova_description)/urdf/kinova_common.xacro" />

   
    <xacro:macro name="kinova_3fingers" params="link_hand">

<!-- 1.310949945 1.150615015 1.149100618 -->
        <xacro:kinova_finger prefix="test" finger_number="1" hand="${link_hand}" finger_origin_xyz="0.00279 0.03126 -0.11467" finger_origin_rpy="-1.830642709 1.150615015 1.149100618"/>
        <xacro:kinova_finger prefix="test" finger_number="2" hand="${link_hand}" finger_origin_xyz="-0.02226 -0.02707 -0.11482" finger_origin_rpy="1.570796327 0.1256637062 1.386140492"/>
        <xacro:kinova_finger prefix="test" finger_number="3" hand="${link_hand}" finger_origin_xyz="0.02226 -0.02707 -0.11482" finger_origin_rpy="0 0 0"/>

        
    </xacro:macro>


</root>